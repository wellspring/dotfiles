style "scrollbars" = "default"
{
engine "pixmap"
{
	image
	{
		function	= BOX
		recolorable	= TRUE
		detail		= "trough"
		file		= "/scrollbar/trough-scrollbar-horiz.png"
		border		= { 11, 11, 6, 6 }
		stretch		= TRUE
