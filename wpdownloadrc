page            = sql.gz
category        = sql.gz
interwiki       = sql.gz
langlinks       = sql.gz
externallinks   = sql.gz
templatelinks   = sql.gz
imagelinks      = sql.gz
categorylinks   = sql.gz
pagelinks       = sql.gz
site_stats      = sql.gz

